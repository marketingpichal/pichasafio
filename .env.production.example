# Configuración de producción - Supabase
# Copia este archivo a .env.production y completa con tus credenciales de producción

VITE_SUPABASE_URL=https://tu-proyecto-prod.supabase.co
VITE_SUPABASE_ANON_KEY=tu_anon_key_de_produccion
SUPABASE_SERVICE_ROLE_KEY=tu_service_role_key_de_produccion

# Configuración de ambiente
NODE_ENV=production
VITE_APP_ENV=production

# Configuración de aplicación
VITE_APP_NAME=Pichasafio
VITE_APP_VERSION=1.0.0

# URLs de producción
VITE_APP_URL=https://tu-dominio.com
VITE_API_URL=https://tu-proyecto-prod.supabase.co

# Configuración de autenticación
VITE_AUTH_REDIRECT_URL=https://tu-dominio.com/auth/callback
VITE_AUTH_RESET_PASSWORD_URL=https://tu-dominio.com/reset-password

# Configuración adicional de producción
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_LOG_LEVEL=error